{
  "expo": {
    "name": "Money_Monitoring",
    "slug": "money-monitoring",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "moneymonitoring",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.money-monitoring.system",
      "statusBarHidden": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.nazarenomarkanthony060120.money_monitoring_system",
      "statusBarHidden": true,
      "navigationBarHidden": true,
      "softwareKeyboardLayoutMode": "pan",
      "allowBackup": false,
      "fullscreen": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e8c634ad-1ee5-4b26-8625-ac74e629211a"
      },
      "googleClientId": "544430917308-uep067du60f23usbje9pueu77a4ds9q0.apps.googleusercontent.com"
    },
    "owner": "nazarenomarkanthony060120"
  }
}
